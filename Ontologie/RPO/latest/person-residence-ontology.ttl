#################################################################
#    Classes
#################################################################

###  https://w3id.org/italia/onto/CLV/Address
CLV:Address rdf:type owl:Class ;
            rdfs:label "Indirizzo"@it .


###  https://w3id.org/italia/onto/CLV/City
CLV:City rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf rpo:currentlyHasDomicileIn
                                              ] ;
                               owl:someValuesFrom owl:Thing
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf rpo:currentlyResidesIn
                                              ] ;
                               owl:someValuesFrom owl:Thing
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf rpo:residenceInCityAtTimeOfDeath
                                              ] ;
                               owl:someValuesFrom owl:Thing
                             ] ;
         rdfs:label "Comune"@it .


###  https://w3id.org/italia/onto/CLV/Country
CLV:Country rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty [ owl:inverseOf rpo:bornInCountry
                                                 ] ;
                                  owl:someValuesFrom owl:Thing
                                ] ;
            rdfs:label "Stato"@it .


